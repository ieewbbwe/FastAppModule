<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">
    <data>
        <variable
            name="viewModel"
            type="com.mobile_core.module_personal.main.MineViewModel"/>

        <variable
            name="toolBarViewModel"
            type="com.mobile_core.base.mvvm.ToolBarViewModel"/>
    </data>

    <android.support.constraint.ConstraintLayout xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.mobile_core.module_personal.main.MineActivity">

        <include
            android:id="@+id/toolBarLayout"
            layout="@layout/tool_bar_default"
            app:viewModel ="@{toolBarViewModel}" />

        <TextView
            android:text="个人—组件"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </android.support.constraint.ConstraintLayout>

</layout>
